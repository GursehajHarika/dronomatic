<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gursehajharika.dronomatic">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main_icon_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".register" />
        <activity android:name=".infomation" />
        <activity android:name=".homepage" />
        <activity android:name=".Loginwelcome" />
        <activity android:name=".temperature" />
        <activity android:name=".baropress" />
        <activity android:name=".Motionsense" />
        <activity android:name=".loginwelcomeadmin" />
        <activity android:name=".Accountinfo" />
        <activity android:name=".contactus" />
        <activity android:name=".settings" />
        <activity android:name=".contactuserror"></activity>
    </application>

</manifest>